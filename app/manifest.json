{
  "name": "Bulk URL Opener",
  "short_name": "BulkOpen",
  "author": "Euan Riggans",
  "manifest_version": 2,
  "version": "1.9.0",
  "description": "A browser addon / extension that provides the user with lots of features to handle lists of urls.",
  "permissions": [
    "tabs"
  ],
  "background": {
    "scripts": [
      "js/utility.js",
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon/64.png",
    "default_title": "Bulk URL Open",
    "default_popup": "popup.html"
  },
  "options_ui": {
    "page": "settings.html",
    "open_in_tab": true
  },
  "icons": {
    "16": "icon/16.png",
    "32": "icon/32.png",
    "64": "icon/64.png",
    "128": "icon/128.png"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+U"
      }
    }
  }
}
